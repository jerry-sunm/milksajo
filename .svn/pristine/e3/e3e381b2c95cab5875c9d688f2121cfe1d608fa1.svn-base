package com.milk.dto;
/*
이름           널?       유형            
------------ -------- ------------- 
U_USER_ID    NOT NULL VARCHAR2(20)  
U_PASSWORD   NOT NULL VARCHAR2(30)  
U_NAME       NOT NULL VARCHAR2(10)  
U_BIRTHDAY            DATE          
U_ADDRESS    NOT NULL VARCHAR2(500) 
U_TEL        NOT NULL VARCHAR2(14)  
U_EMAIL               VARCHAR2(30)  
U_STATE      NOT NULL NUMBER(1)     
U_LAST_LOGIN NOT NULL DATE          
U_JOIN_DATE  NOT NULL DATE          
   
*/
public class UserDTO {
	private String uUserId; // 회원아이디
	private String uPassword; // 회원비밀번호
	private String uName; // 회원이름
	private String uBirthday; // 회원생일
	private String uAddress; //주소 (여러주소를 ';'로 구분)
	private String uTel; // 전화번호
	private String uEmail; // 이메일
	private int uState; // 회원상태 : 0: 휴먼,  1: 정상
	private String uLastLogin; // 마지막 로그인 날짜
	private String uJoinDate; // 가입일
	
	public String getuUserId() {
		return uUserId;
	}
	public void setuUserId(String uUserId) {
		this.uUserId = uUserId;
	}
	public String getuPassword() {
		return uPassword;
	}
	public void setuPassword(String uPassword) {
		this.uPassword = uPassword;
	}
	public String getuName() {
		return uName;
	}
	public void setuName(String uName) {
		this.uName = uName;
	}
	public String getuBirthday() {
		return uBirthday;
	}
	public void setuBirthday(String uBirthday) {
		this.uBirthday = uBirthday;
	}
	public String getuAddress() {
		return uAddress;
	}
	public void setuAddress(String uAddress) {
		this.uAddress = uAddress;
	}
	public String getuTel() {
		return uTel;
	}
	public void setuTel(String uTel) {
		this.uTel = uTel;
	}
	public String getuEmail() {
		return uEmail;
	}
	public void setuEmail(String uEmail) {
		this.uEmail = uEmail;
	}
	public int getuState() {
		return uState;
	}
	public void setuState(int uState) {
		this.uState = uState;
	}
	public String getuLastLogin() {
		return uLastLogin;
	}
	public void setuLastLogin(String uLastLogin) {
		this.uLastLogin = uLastLogin;
	}
	public String getuJoinDate() {
		return uJoinDate;
	}
	public void setuJoinDate(String uJoinDate) {
		this.uJoinDate = uJoinDate;
	}
}